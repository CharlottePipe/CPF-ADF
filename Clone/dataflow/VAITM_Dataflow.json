{
	"name": "VAITM_Dataflow",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "SAP_CDC_VAITM",
						"type": "DatasetReference"
					},
					"name": "InputVAITM"
				}
			],
			"sinks": [
				{
					"linkedService": {
						"referenceName": "charlotte0pipe0metastore",
						"type": "LinkedServiceReference"
					},
					"name": "OutputVAITM",
					"rejectedDataLinkedService": {
						"referenceName": "charlotte0pipe0metastore",
						"type": "LinkedServiceReference"
					}
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     staged: true,",
				"     readMode: 'fullAndIncrementalLoad',",
				"     keys: ['VBELN','POSNR'],",
				"     format: 'sapObject',",
				"     fixDecimalPrecision: true) ~> InputVAITM",
				"InputVAITM sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'delta',",
				"     compressionType: 'snappy',",
				"     compressionLevel: 'Fastest',",
				"     fileSystem: 'raw-data',",
				"     folderPath: 'HR_Analytics/VAITM',",
				"     overwrite: true,",
				"     mergeSchema: false,",
				"     autoCompact: false,",
				"     optimizedWrite: false,",
				"     vacuum: 0,",
				"     deletable: true,",
				"     insertable: true,",
				"     updateable: true,",
				"     upsertable: true,",
				"     keys:['VBELN','POSNR'],",
				"     umask: 0666,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     outputAssertFailedRows: true,",
				"     assertFailure_fileSystem: 'failed-records') ~> OutputVAITM"
			]
		}
	}
}