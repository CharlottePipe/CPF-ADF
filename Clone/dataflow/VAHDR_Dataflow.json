{
	"name": "VAHDR_Dataflow",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "Sap_CDC_VAHDR",
						"type": "DatasetReference"
					},
					"name": "InputVAHDR"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "VAHDR_Parquet",
						"type": "DatasetReference"
					},
					"name": "OutputVAHDR",
					"rejectedDataLinkedService": {
						"referenceName": "charlotte0pipe0metastore",
						"type": "LinkedServiceReference"
					}
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     staged: true,",
				"     readMode: 'fullAndIncrementalLoad',",
				"     keys: ['VBELN'],",
				"     format: 'sapObject',",
				"     fixDecimalPrecision: true) ~> InputVAHDR",
				"InputVAHDR sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'parquet',",
				"     umask: 0666,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     outputAssertFailedRows: true,",
				"     assertFailure_fileSystem: 'failed-records') ~> OutputVAHDR"
			]
		}
	}
}